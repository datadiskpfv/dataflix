<header>
  <h1>New Postcode</h1>
</header>

<%= render 'form', postcode: @postcode %>

<% if @postcode.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@postcode.errors.count, 'error') %>
      prohibited this film from being created: </h2>

    <ul>
      <% @postcode.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>